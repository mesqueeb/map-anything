{
  "name": "map-anything",
  "version": "3.1.1",
  "description": "Array.map but for objects with good TypeScript support. A small and simple integration.",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "vitest run",
    "lint": "tsc --noEmit && eslint ./src",
    "build": "del-cli dist && tsc",
    "release": "npm run lint && npm run build && np"
  },
  "devDependencies": {
    "@cycraft/eslint": "^0.4.3",
    "@cycraft/tsconfig": "^0.1.2",
    "del-cli": "^6.0.0",
    "np": "^10.2.0",
    "vitest": "^3.0.6"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "map-object",
    "object-map",
    "object-to-object",
    "mapping",
    "transform",
    "object-mapper",
    "compose",
    "map-reduce"
  ],
  "author": "Luca Ban - Mesqueeb (https://cycraft.co)",
  "funding": "https://github.com/sponsors/mesqueeb",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mesqueeb/map-anything.git"
  },
  "homepage": "https://github.com/mesqueeb/map-anything#readme",
  "bugs": "https://github.com/mesqueeb/map-anything/issues"
}
